package Order;

/**
 * @author admin
 *
 */
public class test {

	public test() {
		changeAge(12);
	}


	int age=10;
	
	int changeAge(int newa) {
		age=newa;
		return age;
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		test t=new test();
		System.out.println(t.age);	
		
	}

}
